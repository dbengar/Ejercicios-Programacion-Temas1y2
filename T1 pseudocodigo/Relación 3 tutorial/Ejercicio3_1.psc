//Crea un programa que escriba el resultado de multiplicar 
//los dos números que introduzca el usuario

Algoritmo Ejercicio2_1
	
	Definir num1 Como Real
	Definir num2 Como Real
	
	Escribir "Introduce el primer número"
	Leer num1
	Escribir "Introduce el segundo número"
	Leer num2
	Escribir "El resultado de multiplicar",num1," por ",num2," es ",num1*num2;
	
	
FinAlgoritmo
